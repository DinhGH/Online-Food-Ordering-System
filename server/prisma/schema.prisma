generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Food {
  id        Int     @id @default(autoincrement())
  name      String
  image     String
  price     Float
  desc      String?
}

model Order {
  id        Int      @id @default(autoincrement())
  items     Json
  total     Float
  createdAt DateTime @default(now())
  paymentId String?
}
